<launch>

	<arg name="stereo" default="/stereo_forward"/>
	<arg name="camera" default="/stereo_forward/left"/>
  <arg name="image" default="image_rect_color"/>

	<include file="$(find triton_launchfiles)/launch/valve_tracker.launch">
		<arg name="stereo" value="$(arg stereo)"/>
		<arg name="image" value="$(arg image)"/>
	</include>

	<include file="$(find triton_launchfiles)/launch/ar_marker_detector.launch">
		<arg name="camera" value="$(arg camera)"/>
		<arg name="image" value="$(arg image)"/>
	</include>

	<include file="$(find triton_launchfiles)/launch/template_pose_estimation.launch">
		<arg name="camera" value="$(arg camera)"/>
		<arg name="image" value="$(arg image)"/>
	</include>

</launch>